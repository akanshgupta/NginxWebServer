events {}

http {
    include mime.types;
    server {
        listen 80;
        server_name 18.222.167.21;

        root /var/www/html;

        try_files $uri /cat.css /greet @friendly_404;

        location @friendly_404 {
            return 404 "Sorry, this page cannot be found!";
        }
        
        location /greet {
            return 200 "Welcome to the club!";
        }
    }  
}
